else 
	busybox chmod 644 /sys/module/lowmemorykiller/parameters/minfree
	busybox echo "\$lmk19,\$lmk20,\$lmk21,\$lmk22,\$lmk23,\$lmk24" > /sys/module/lowmemorykiller/parameters/minfree
fi

if [ -e /sys/module/lowmemorykiller/parameters/cost ];then
	busybox chmod 644 /sys/module/lowmemorykiller/parameters/cost
	busybox echo "64" > /sys/module/lowmemorykiller/parameters/cost
fi

		busybox clear
		busybox echo "Proses Normal selesai" | tee -a \$ngz
		busybox sleep 0.5
		busybox echo "" | tee -a \$ngz
		busybox echo "\$(date +"%d-%Y %H:%M" ) Pemasangan Normal berhasil" | tee -a \$ngz
		busybox sleep 2
		busybox clear
lmkper
		busybox clear
		busybox echo "Proses selesai, segera reboot..."
		busybox echo ""
     busybox sleep 1
		busybox echo "kembali..."
		busybox sleep 2
		busybox clear 		
		sh /system/bin/11ngz
		fi
		fi

     if [ $lmkt -eq 4 ];then
		busybox clear
		sh /system/etc/NGZ/00NGZramz
		else
		sh /system/bin/11ngz
		fi


		if [ $lmkt -eq 5 ];then
		busybox clear
		sh /system/bin/11ngz
		fi
;;

5)
     busybox clear
     busybox chmod 755 /system/etc/NGZ/00NGZgpdfix
     sh /system/etc/NGZ/00NGZgpdfix
     busybox echo ""
     busybox echo -e $garis1
     busybox echo -e "Value : $gps"
     busybox echo -e $garis1
     busybox echo ""
     busybox echo -n "Aktifkan saat reboot ? (yes/no) : "
     read menu

     if [ $menu = yes ];then

if [ -e /system/etc/init.d/00NGZgpdfix ];then
busybox rm -f /system/etc/init.d/00NGZgpdfix
fi
     busybox cp /system/etc/NGZ/00NGZgpdfix /system/etc/init.d
     busybox chmod 755 /system/etc/init.d/00NGZgpdfix
     busybox echo ""
     busybox echo "Proses selesai, kembali..."
     busybox sleep 2
     busybox clear
     sh /system/bin/11ngz 

elif [ $menu = no ];then

if [ -e /system/etc/init.d/00NGZgpdfix ];then
busybox rm -f /system/etc/init.d/00NGZgpdfix
fi
     busybox echo ""
     busybox echo "Proses selesai, kembali..."
     busybox sleep 2
     busybox clear
     sh /system/bin/11ngz 
else
     busybox echo "Input yang anda masukan salah, harap ulangi lagi"
fi

     busybox echo "Proses selesai, kembali..."
     busybox sleep 2
     busybox clear
     sh /system/bin/11ngz 
;;

6)
busybox clear
heaplog=/system/etc/NGZ/ngzlog/heap.log
rm -f $heaplog
touch $heaplog
busybox echo -e $garis1;
busybox echo -e "Value anda : \e[0;35m `getprop dalvik.vm.heapsize` \e[00;37;40m"
busybox echo -e $garis1;
busybox echo "" 
busybox echo -e "1. Normal = \e[0;36m512\e[00;37;40m"
busybox echo -e $garis2;
busybox echo -e "2. Smooth = \e[0;36m384\e[00;37;40m"
busybox echo -e $garis2;
busybox echo -e "3. Lazy   = \e[0;36m256\e[00;37;40m"
busybox echo -e $garis2;
busybox echo -e "4. Gacor  = \e[0;36m48\e[00;37;40m"
busybox echo -e $garis2;
busybox echo -e "5. Gokil  = \e[0;36m32\e[00;37;40m"
busybox echo -e $garis2;
busybox echo -e "6. Sadis  = \e[0;36m24\e[00;37;40m"
busybox echo -e $garis2;
busybox echo "7. Kembali"
busybox echo -e $garis2;
busybox echo -n "Masukan pilihan (1-7) : "
read menu

if [ $menu -eq 1 ];then
#Normal
busybox clear
setprop dalvik.vm.heapsize 512m
heap=`getprop dalvik.vm.heapsize | cut -dm -f1`;
tar=`getprop dalvik.vm.heaptargetutilization | cut -dm -f1`;
kal=`awk -v h=$heap -v t=$tar 'BEGIN { print (t / 3) * h }'`;
setprop dalvik.vm.heapgrowthlimit $kal'm';
busybox echo "$(date +"%d-%Y %H:%M") Proses Heap Normal berhasil" | tee -a $heaplog
busybox echo ""
busybox echo "Value anda : `getprop dalvik.vm.heapgrowthlimit` dan `getprop dalvik.vm.heapsize`" | tee -a $heaplog
busybox sleep 2
busybox clear
sh /system/bin/11ngz
elif [ $menu -eq 2 ];then
# Smooth
busybox clear
setprop dalvik.vm.heapsize 384m
heap=`getprop dalvik.vm.heapsize | cut -dm -f1`;
tar=`getprop dalvik.vm.heaptargetutilization | cut -dm -f1`;
kal=`awk -v h=$heap -v t=$tar 'BEGIN { print (t / 3) * h }'`;
setprop dalvik.vm.heapgrowthlimit $kal'm';

busybox echo "$(date +"%d-%Y %H:%M") Proses Heap Smooth berhasil" | tee -a $heaplog
busybox echo ""
busybox echo "Value anda : `getprop dalvik.vm.heapgrowthlimit` dan `getprop dalvik.vm.heapsize`" | tee -a $heaplog
busybox sleep 2
busybox clear
sh /system/bin/11ngz
elif [ $menu -eq 3 ];then
#Lazy
busybox clear
setprop dalvik.vm.heapsize 256m
heap=`getprop dalvik.vm.heapsize | cut -dm -f1`;
tar=`getprop dalvik.vm.heaptargetutilization | cut -dm -f1`;
kal=`awk -v h=$heap -v t=$tar 'BEGIN { print (t / 3) * h }'`;
setprop dalvik.vm.heapgrowthlimit $kal'm';

busybox echo "$(date +"%d-%Y %H:%M") Proses Heap Lazy berhasil" | tee -a $heaplog
busybox echo ""
busybox echo "Value anda : `getprop dalvik.vm.heapgrowthlimit` dan `getprop dalvik.vm.heapsize`" | tee -a $heaplog
busybox sleep 2
busybox clear
sh /system/bin/11ngz
elif [ $menu -eq 4 ];then
#hard
busybox clear
setprop dalvik.vm.heapsize 48m
heap=`getprop dalvik.vm.heapsize | cut -dm -f1`;
tar=`getprop dalvik.vm.heaptargetutilization | cut -dm -f1`;
kal=`awk -v h=$heap -v t=$tar 'BEGIN { print (t / 3) * h }'`;
setprop dalvik.vm.heapgrowthlimit $kal'm';

busybox echo "$(date +"%d-%Y %H:%M") Proses Heap Gacor berhasil" | tee -a $heaplog
busybox echo ""
busybox echo "Value anda : `getprop dalvik.vm.heapgrowthlimit` dan `getprop dalvik.vm.heapsize`" | tee -a $heaplog
busybox sleep 2
busybox clear
sh /system/bin/11ngz
elif [ $menu -eq 5 ];then
#crazy
busybox clear
setprop dalvik.vm.heapsize 32m
heap=`getprop dalvik.vm.heapsize | cut -dm -f1`;
tar=`getprop dalvik.vm.heaptargetutilization | cut -dm -f1`;
kal=`awk -v h=$heap -v t=$tar 'BEGIN { print (t / 3) * h }'`;
setprop dalvik.vm.heapgrowthlimit $kal'm';

busybox echo "$(date +"%d-%Y %H:%M") Proses Heap Gokil berhasil" | tee -a $heaplog
busybox echo ""
busybox echo "Value anda : `getprop dalvik.vm.heapgrowthlimit` dan `getprop dalvik.vm.heapsize`" | tee -a $heaplog
busybox sleep 2
busybox clear
sh /system/bin/11ngz
elif [ $menu -eq 6 ];then
#insane
busybox clear
setprop dalvik.vm.heapsize 24m
heap=`getprop dalvik.vm.heapsize | cut -dm -f1`;
tar=`getprop dalvik.vm.heaptargetutilization | cut -dm -f1`;
kal=`awk -v h=$heap -v t=$tar 'BEGIN { print (t / 3) * h }'`;
setprop dalvik.vm.heapgrowthlimit $kal'm';

busybox echo "$(date +"%d-%Y %H:%M") Proses Heap Sadis berhasil" | tee -a $heaplog
busybox echo ""
busybox echo "Value anda : `getprop dalvik.vm.heapgrowthlimit` dan `getprop dalvik.vm.heapsize`" | tee -a $heaplog
busybox sleep 2
busybox clear
sh /system/bin/11ngz
elif [ $menu -eq 7 ];then
busybox sleep 1
busybox clear
sh /system/bin/11ngz
else
busybox clear
busybox echo "Maaf, ulangi lagi"
busybox sleep 1.5
sh /system/bin/11ngz
fi;;

7)
if [ -e /system/bin/mpdecision ];then
busybox clear
busybox echo "Mengaktifkan MPDECISION"
busybox sleep 1.5
busybox mv /system/bin/mpdecision /system/etc/NGZ
busybox echo ""
busybox echo "Proses selesai, kembali"
busybox sleep 1.5
busybox clear
sh /system/bin/11ngz
elif [ -e /system/etc/NGZ/mpdecision ];then
busybox clear
busybox echo "Menonakifkan MPDECISION"
busybox sleep 1.5
busybox mv /system/etc/NGZ/mpdecision /system/bin/
busybox chmod 755 /system/bin/mpdecision
busybox echo ""
busybox echo "Proses selesai, kembali"
busybox sleep 1.5
busybox clear
sh /system/bin/11ngz
else
busybox clear
busybox echo "Maaf, kernel tidak support mpdecision, kembali"
busybox sleep 2
busybox clear
sh /system/bin/11ngz
fi
;;

8)
	sh /system/bin/10ngz
	exit
;;

*)
	busybox clear
	busybox echo "Inputan salah, ulangi lagi"
	busybox sleep 2
	busybox clear
	sh /system/bin/11ngz
;;
esac
exit 0